    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-session": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "http-status-code": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino-pretty": "^13.1.1",
    "redis": "^5.8.2",
